{
    "id": "scenario-loader",
    "version": "1.0.0",
    "targetMinskyVersion": "3.20.0",
    "contributions": {
        "menus": [
            {
                "targetMenu": "simulation",
                "label": "Load Scenario...",
                "route": "simulation/load-scenario",
                "window": {
                    "width": 800,
                    "height": 600,
                    "title": "Load Scenario"
                }
            }
        ],
        "routes": [
            {
                "path": "load-scenario",
                "moduleName": "ScenarioLoaderModule"
            }
        ],
        "ipc": [
            {
                "channel": "read-file-text",
                "description": "Reads a text file from the filesystem. Required for large CSV imports."
            }
        ]
    }
}